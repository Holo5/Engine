{
  "name": "@holo5/holo5",
  "version": "0.3.0",
  "description": "Holo5 Power !",
  "keywords": [
    "holo5",
    "habbo",
    "extractor",
    "pixijs"
  ],
  "homepage": "https://github.com/Holo5/Holo5-Client#readme",
  "bugs": {
    "url": "https://github.com/Holo5/Holo5-Client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Holo5/Holo5-Client.git"
  },
  "license": "UNLICENSED",
  "author": "Pi-Bouf",
  "scripts": {
    "client-start": "parcel src/index.html --log-level 4",
    "static-serve": "http-server Client/Static/ -p 8081 -i --cors",
    "habbo-spritesheet-generator": "node assets/sass/habbo/images/util/SpriteGenerator.js",
    "lint": "eslint --ext .js,.ts ./src --color",
    "build": "yarn build:conf && yarn build:parcel && yarn build:obfuscate",
    "build:conf": "rm src/conf.ts && cp src/conf.prod.ts src/conf.ts",
    "build:parcel": "parcel build src/index.html --no-cache --no-source-maps",
    "build:obfuscate": "javascript-obfuscator $(ls ./dist/main.*.js) --output $(ls ./dist/main.*.js)"
  },
  "dependencies": {
    "@holo5/hotelview-intl": "^0.0.4",
    "@holo5/roombuilder": "0.4.2",
    "@yaireo/console-colors": "^1.1.0",
    "bytebuffer": "^5.0.1",
    "core-js": "3",
    "gsap": "^3.8.0",
    "mementojs": "^1.4.0",
    "pixi.js": "6.1.2",
    "ts-bus": "^2.3.1",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@types/bytebuffer": "^5.0.42",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.2",
    "javascript-obfuscator": "^3.0.0",
    "parcel": "1.12.3",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "vue-class-component": "^7.2.6",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.14"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js",
    "Mixins": "./assets/sass/_mixins.scss",
    "Habbo": "./assets/sass/habbo"
  }
}
